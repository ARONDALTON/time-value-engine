    "build": "tsc",
    "tsc:w": "tsc -w",
    "postinstall": "npm run tsc",
    "lint": "tslint **/*.ts --e **/*.d.ts"
  },
  "keywords": [
    "time value",
    "amortize",
    "discounted cash flows",
    "amortization"
  ],
  "author": "kevinsherman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kevinsherman/time-value-engine/issues"
  },
  "main": "dist/index.js",
  "types": "./index.d.ts",
  "homepage": "https://github.com/kevinsherman/time-value-engine#readme",
  "devDependencies": {
    "@types/jasmine": "^2.5.43",
    "concurrently": "^3.4.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "rimraf": "^2.6.0",
    "tslint": "^4.4.2",
    "typescript": "^2.2.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "moment": "^2.17.1"
