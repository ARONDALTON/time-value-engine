{
  "name": "time-value-engine",
  "version": "0.0.4",
  "description": "Time Value of Money Calculator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevinsherman/time-value-engine.git"
  },
  "scripts": {
    "test": "concurrently 'npm run tsc:w' 'karma start karma.conf.js'",
    "clean": "rimraf dist/*",
